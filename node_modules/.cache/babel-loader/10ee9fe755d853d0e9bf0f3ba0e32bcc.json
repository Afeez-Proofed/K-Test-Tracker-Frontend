nd: #fafafa\n    color: #6174a0\n    transition: 1s\n    margin: 0\n\n.drop-down ul\n    margin: 8px 0 0 0\n    list-style: none\n    padding: 0\n    text-decoration: none\n    transition: 1s\n\n.drop-down li\n  padding: 6px 0\n  border-top: 1px dashed lightgrey\n  list-style-type: none\n  text-align: center\n  transition: 0.33s ease\n\n.drop-down li:hover\n  color: #008F5C\n</style>\n"],"sourceRoot":"src/components"}]}